	Keywords = {Public Opinion},
	Month = {January},
	Number = {1},
	Pages = {169-183},
	Title = {Money, Time, and Political Knowledge: Distinguishing Quick Recall and Political Learning Skills},
	Volume = {52},
	Year = {2008}}

