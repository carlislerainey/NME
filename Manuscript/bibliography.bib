	Date-Added = {2009-11-30 08:16:09 -0500},
	Date-Modified = {2009-11-30 10:07:11 -0500},
	Keywords = {Political Institutions, summary, comparative},
	Publisher = {Cambridge University Press},
	Title = {Deliberate Discretion? The Institutionial Foundations of Bureaucratic Autonomy},
	Year = {2002}}

